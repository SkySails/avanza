{
  "name": "avanza",
  "version": "2.0.0",
  "description": "A wrapper for the unofficial Avanza API",
  "main": "dist/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production babel lib -d dist && tsc",
    "test": "npm start && cross-env NODE_ENV=test NODE_PATH=lib ava tests/**/*.test.js",
    "lint": "eslint lib/**/*",
    "docs": "npm start && node scripts/generate-documentation.js",
    "prepublishOnly": "npm run lint",
    "prepare": "npm run start"
  },
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SkySails/avanza/.git"
  },
  "author": "Filip Hallqvist <filiphallqvist@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fhqvst/avanza/issues"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/fhqvst/avanza#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "crypto-js": "^3.3.0",
    "querystring": "^0.2.0",
    "ws": "3.2.0"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "babel-cli": "^6.24.0",
    "babel-preset-env": "^1.6.0",
    "cross-env": "^5.0.5",
    "documentation": "^4.0.0-rc.1",
    "dotenv": "^2.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.12.0",
    "jsdoc": "^3.6.6",
    "sinon": "^2.3.6",
    "tsd-jsdoc": "^2.5.0",
    "typescript": "^4.1.3"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": "last 2 versions",
            "node": "6.11"
          }
        }
      ]
    ]
  }
}
